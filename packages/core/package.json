{
  "name": "qilin-ui",
  "version": "1.0.0",
  "description": "Components library by Vue3 + TS, This cool",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/core/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.umd.cjs",
      "types": "./dist/types/core/index.d.ts"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": ["./dist/index.css"],
  "scripts": {
    "build": "run-s clean build-only move-style",
    "build-only": "run-p build-esm build-umd",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-esm": "vite build --config vite.esm.config.ts",
    "move-style": "move-file dist/esm/index.css dist/index.css",
    "clean": "rimraf dist",
    "release": "release-it"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "devDependencies": {
    "vite-plugin-dts": "^3.9.1",
    "@qilin-ui/components": "workspace:*"
  },
  "peerDependencies": {
    "vue": "^3.4.21"
  }
}
