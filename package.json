{
  "name": "qilin-ui-monorepo",
  "version": "1.0.0",
  "type": "module",
  "main": "index.ts",
  "scripts": {
    "dev": "pnpm run build && pnpm --filter @qilin-ui/playground dev",
    "story": "pnpm run build && pnpm --filter @qilin-ui/playground storybook",
    "test": "cross-env NODE_ENV=test pnpm --filter @qilin-ui/components test",
    "build": "cross-env NODE_ENV=production pnpm --filter qilin-ui build",
    "build:dev": "cross-env NODE_ENV=development pnpm --filter qilin-ui build:watch",
    "docs:dev": "pnpm --filter @qilin-ui/docs dev",
    "docs:build": "pnpm --filter @qilin-ui/docs build",
    "docs:preview": "pnpm --filter @qilin-ui/docs preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "packages/*"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.2",
    "@fortawesome/free-solid-svg-icons": "^6.5.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@popperjs/core": "^2.11.8",
    "@qilin-ui/hooks": "workspace:*",
    "@qilin-ui/theme-chalk": "workspace:*",
    "@qilin-ui/utils": "workspace:*",
    "async-validator": "^4.2.5",
    "lodash-es": "^4.17.21",
    "qilin-ui": "workspace:*",
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.24.5",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.20",
    "@types/shelljs": "^0.8.15",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "cross-env": "^7.0.3",
    "eslint": "8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^6.0.1",
    "prettier": "3.2.5",
    "release-it": "^17.3.0",
    "sass": "^1.77.1",
    "shelljs": "^0.8.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.4.0",
    "vue-tsc": "^2.0.6"
  }
}
